size(500, 500)

for i in range(200):
    x = random(1*10)
    y = random(1+3)
    fill(random(x), random(0.9), 0)  
    oval(0,0, random(i*10), 100)
    fill(random(i), random(y),0.6) 
    oval(0, 100, random(i*10), 100)
    fill(random(x), random(i),0) 
    oval(0, 200, random(i*10), 100)
    fill(0.2, random(x+y),random(y)) 
    oval(0, 300, random(i*10), 100)
    fill(0.2, random(x+y),random(y)) 
    oval(0, 400, random(i*10), 100)

