import noise

def setup():
    global t
    size(200, 200)
    background("#009", "aqua")
    t = 0

def draw():
    global t
    for y in range(0, WIDTH, 10):
        for x in range(0, WIDTH, 10):
            z = noise.snoise3((x * 0.01), (y * 0.01), t, octaves=4)
            z = z * 500
            x_3d = x * 0.5 + y * 0.5
            y_3d = (WIDTH / 2) + (y * 0.3) - (x * 0.2) - (z * 0.5)
            rect(x_3d, y_3d, 15, 2)
    t = t + 0.05