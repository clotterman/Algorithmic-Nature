import noise

def setup():
    global t
    size(200, 200)
    background(1)
    t = 0

def draw():
    global t
    for y in range(0, HEIGHT, 10):
        for x in range(0, WIDTH, 10):
            z = noise.snoise3((x * 0.01), (y * 0.01), (t * 0.02), octaves=4)
            fill(0.5 + (z * 0.5))
            oval(x, y, 50, 50)
            oval(x, y, 50, 50)
    t = t + 5