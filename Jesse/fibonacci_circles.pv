size(200, 200)
background(0)

def fib(n):
    if n < 2:
        return 1
    return fib(n - 1) + fib(n - 2)
    
stroke (10)
fill (10,0.2, 0.3)
y =30
for i in range(15):
    # variable s to ooptimize for speed
    s = fib(i)
    ellipse(40, s, 80, s)
    y += s # same as y = y+s
    
print range (5, -1)
