size(200, 200)
background(0)
words = ["+-+-+-+-"]
colors = [color(0), color(1,1,1), color(1,1,1)]

for i in range(40):
    x = random(WIDTH)
    y = random(HEIGHT)
    fontsize(random(10,50))
    rotate(random(60))

    fill(choice(colors))
    text(choice(words), x, y)
    
    export('random_6.png')