size(200, 200)
background(1)

def fib (n):
    if n < 2:
        return 1
    return fib(n - 1) + fib(n - 2)
    
stroke(0)
fill(1, 1, 1)
background(0)
x = 50
y = 10
for i in range(15):
    s = fib(i)
    print"%d: %d" % (i + 1, s)
    rect(x, y, s, s)
    y += s
    
    export('Fibonacci_2.png')
    
    