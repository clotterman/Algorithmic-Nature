import noise
global t

def setup():
    size(200, 200)
    global t
    background(0.5, 0.3, 0.6)
    t = 2

def draw():
    global t
    stroke(5)
    t = t + 1
    for x in range(HEIGHT):
        y = noise.snoise2((x * 0.001) + t, 0.2, octaves=(10))
        y *= WIDTH / 3.0
        y += WIDTH / 2.0
        line(x, HEIGHT, x, WIDTH - y)