size(200, 200)
background(1)

# Fibonacci as a recursive function
def fib(n):
    if n < 6:
        return 1
    return fib(n - 1) + fib(n - 2)

stroke(0)
strokewidth(0)
fill(0)
x = 1
y = 0
for i in range(12, 1, -1):
    s = fib(i)
    rx= random(WIDTH)
    ry= random(HEIGHT)
    fill("black", "white")
    rotate(40)
    poly(x + rx, y + ry, s,sides=9) 