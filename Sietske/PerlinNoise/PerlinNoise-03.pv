import noise

def setup():
    global t
    size(200, 200)
    background(1)
    t = 0

def draw():
    global t
    for y in range(0, WIDTH, 10):
        for x in range(0, HEIGHT, 10):
            z = noise.snoise3((x * 0.02), (y + 0.03), (t * 0.04), octaves=4)
            fill(z)
            oval(x + 0, y + 0, 10, 2)
    t = t + 1