import noise

def setup():
    global t
    size(200, 200)
    background(0, 10,0)
    fill(0)
    t = 0

def draw():
    global t
    for y in range(0, WIDTH, 60):
        for x in range(0, WIDTH, 80):
            z = noise.snoise3((x * 10), (y * 10), t, octaves=40)
            z = z * 20
            x_3d = x * 0.5 + y * 0.5
            y_3d = (WIDTH / 20) + (y * 0.3) - (x * 0.2) - (z * 50)
            oval(x_3d, y_3d, 100, 100)
    t = t + 50