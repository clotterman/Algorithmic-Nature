import noise

def setup():
    global t
    size(200, 200)
    background(1, 0, 100)
    t = 100

def draw():
    global t
    for y in range(0, WIDTH, 15):
        for x in range(0, WIDTH, 15):
            z = noise.snoise3((x * 100), (y * 10), (t * 10), octaves=40)
            fill(z)
            arc(x + 5, y + 5, 20)
    t = t + 100