size(200, 200)
background("grey")
words = ["Esmee", "en", "dit"]
colors = [color("black"), color("darkgray"), color("white")]

for i in range(40):
    x = random(WIDTH)
    y = random(HEIGHT)
    rotate(random(360))
    fontsize(random(50,100))

    fill(choice(colors))
    text(choice(words), x, y)