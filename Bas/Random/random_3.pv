size(200, 200)
background('white', 'black')

for i in range(20):
    x = random(WIDTH)
    y = random(HEIGHT)
    fill('white')
    rotate(0)
    rect(x, y, 50, 50)
    
  
   
    fill('black','white')
    rect(x+y, y+x, 50, 50)
    
    
    fill('black', 'white')
    rect(x+x, y+y, 50, 50)
   
    
  
    line(x, y+x, 275, 2);
    
   

