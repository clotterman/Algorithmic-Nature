size(200, 200)
background('white')

for i in range(20):
    x = random(WIDTH)
    y = random(HEIGHT)
    fill('black')
    rotate(0)
    rect(x, y, 7, 50)
    stroke('black')
  
   
    fill('yellow')
    rect(x+y, y+x, 10, 50)
    stroke('black')
    
    fill('red')
    rect(x+x, y+y, 10, 50)
    stroke('black')
    
  
    line(x, y, 100, 100);
    
    
    
   


