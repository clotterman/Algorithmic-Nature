import noise
global t

def setup():
    size(200, 200)
    global t
    background(0.4,0.5,1)
    t = 1

def draw():
    global t
    stroke(10)
    t = t + 0.50
    for x in range(WIDTH):
        y = noise.snoise2((x * 4.01) + t, 0.0, octaves=5)
        y *= HEIGHT / 1.0
        y += HEIGHT / 10.0
        y += HEIGHT / 1.0
        
        line(x+y, HEIGHT, x, HEIGHT - y)