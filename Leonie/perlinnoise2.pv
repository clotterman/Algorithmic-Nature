import noise

def setup():
    global t
    size(200, 200)
    background(1)
    fill(0)
    t = 0

def draw():
    global t
    for y in range(0, WIDTH, 10):
        for x in range(0, WIDTH, 10):
            z = noise.snoise3((x * 0.01), (x * 0.01), t, octaves=4)
            y = y * 20
            x_3d = x * 0.5 + y * 0.5
            y_3d = (WIDTH / 2) + (y * 0.3) - (x * 0.2) - (z * 0.5)
            oval(x_3d, y_3d, 45, 1 * 5 )
    t = t + 0.05