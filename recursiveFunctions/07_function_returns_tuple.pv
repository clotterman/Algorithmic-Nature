size(400, 400)
background(1)
stroke (0)

# start position
x = 200
y = 200

def segment(x, y, deviation):
    x2 = x + random(deviation) - (deviation / 2)
    y2 = y + random(deviation) - (deviation / 2)
    line (x, y, x2, y2)
    return (x2, y2)

for i in range(100):
    (x, y) = segment(x, y, 40)
